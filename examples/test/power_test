is_number(z).
is_number(s(X)) :- s(X).
add(z, X, X).
add(s(X), Y, s(Z)) :- add(X, Y, Z).
multiply(z, _, z).
multiply(s(X), Y, Z) :-
	multiply(X, Y, Z0),
	add(Z0, Y, Z).
power(_, z, s(z)).
power(X, s(Y), Z) :-
	power(X, Y, Z0),
	multiply(Z0, X, Z).
s(X) :- is_number(X).
goal() :- power(s(s(s(s(z)))), s(s(s(s(z)))), Z).
